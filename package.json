    "test": "NODE_ENV=test mocha test"
  },
  "dependencies": {
    "lodash": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^1.8.0",
    "eslint-config-seegno": "^1.1.1",
    "eslint-plugin-babel": "^2.1.1",
    "github-changes": "^1.0.0",
    "jscs": "^2.5.0",
    "jscs-config-seegno": "^1.0.1",
    "mocha": "^2.0.1",
    "pre-commit": "^1.0.10",
    "should": "^7.1.1"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  }
